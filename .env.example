# Environment Variables
REDIS_URL=redis://redis:6379/0

# PostgreSQL Configuration for Caching
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/facial_processing
DEBUG_SQL=false

# Prometheus & Rich Logging Configuration
ENABLE_METRICS=true
LOG_LEVEL=INFO

# Rich Terminal Support
TERM=xterm-256color
COLORTERM=truecolor
FORCE_COLOR=1

# Application Configuration
APP_NAME=Facial Region SVG Service
APP_VERSION=1.0.0

# Cache Configuration
CACHE_TTL_HOURS=24
CACHE_CLEANUP_INTERVAL=3600